{
  "attention": "this base image is overloaded with any user changes persisted to nedb",
  "bcp": {
    "switch": {
      "d": "staging",
      "S": "172.17.0.1"
    }
  },
  "cache": {
    "batch": [],
    "batchHistory": {}
  },
  "docker": {
    "repo": "microsoft/mssql-server-linux",
    "hostPort": 1433,
    "sqlPort": 1433,
    "sqlVolume": "/var/opt/mssql"
  },
  "odbc": {
    "path": "/opt/mssql-tools/bin",
    "pool": {
      "max": 10,
      "min": 0,
      "idleTimeoutMillis": 30000
    },
    "retry": {
      "count": 2,
      "wait": 10000
    }
  },
  "mssql": {
    "acceptEULA": "Y",
    "backup": {
      "filter": ".bak",
      "path": "/var/opt/mssql/data"
    },
    "data": {
      "filter": ".mdf",
      "path": "/var/opt/mssql/data"
    },
    "dump": {
      "filter": "core",
      "path": "/var/opt/mssql/log"
    },
    "log": {
      "filter": ".ldf",
      "path": "/var/opt/mssql/data"
    },
    "conf": "/opt/mssql/lib/mssql-conf/mssql-conf.py",
    "sa": {
      "name": "sa",
      "password": "<YourStrong!Passw0rd>"
    }
  },
  "pem": {
    "days": 1,
    "selfSigned": true
  },
  "sqlcmd": {
    "prefix": [
      "SET ARITHABORT ON;",
      "SET NOCOUNT ON;",
      "SET QUOTED_IDENTIFIER OFF;"
    ],
    "switch": {
      "S": "172.17.0.1",
      "d": "tempdb",
      "r": "r",
      "I": "I",
      "s": "  "
    }
  },
  "sqlpad": {
    "dir": "sqlpaddata",
    "ip": "127.0.0.1",
    "port": 8754,
    "https-port": 8755,
    "baseUrl": "",
    "passphrase": "yet-aN0ther_pA$$werd",
    "cert-passphrase": "",
    "key-path": "private/privateKey.pem",
    "cert-path": "private/cert.pem",
    "admin": "bwunder@yahoo.com",
    "google-client-id": "",
    "google-client-secret": "",
    "disable-userpass-auth": false,
    "allow-csv-download": true,
    "editor-word-wrap": false,
    "query-result-max-rows": 50000,
    "slack-webhook": "",
    "show-schema-copy-button": false,
    "table-chart-links-require-auth": true,
    "public-url": "",
    "smtp-from": "",
    "smtp-host": "",
    "smtp-port": "",
    "smtp-secure": true,
    "smtp-user": "",
    "smtp-password": "",
    "whitelisted-domains": ""
  },
  "vantage": {
    "auth": {
      "deny": 1,
      "retry": 3,
      "retryTime": 500,
      "unlockTime": 3000,
      "users": [
        {
          "user": "jack",
          "pass": "cheese"
        },
        {
          "user": "jill",
          "pass": "crackers"
        }
      ]
    },
    "vorpalCLI": {
      "batchPath": "history",
      "printDate": false,
      "scriptPath": "scripts",
      "tailPath": "/usr/bin"
    },
    "firewall": {
      "policy": "REJECT",
      "rules": [
        { "rule": "accept",
          "ip": "192.168.0.0",
          "subnet": 24
        },
        { "rule": "accept",
          "ip": "127.0.0.1",
          "subnet": 32
        }
      ]
    },
    "loglevel": 20,
    "middleware": "basic",
    "port": 8753
  }
