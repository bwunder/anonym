{"name":"memoryAvailable","text":"SELECT total_physical_memory_kb/1024 AS [Physical Memory (MB)],\n      available_physical_memory_kb/1024 AS [Available Memory (MB)],\n      total_page_file_kb/1024 AS [Total Page File (MB)],\n          available_page_file_kb/1024 AS [Available Page File (MB)],\n          system_cache_kb/1024 AS [System Cache (MB)],\n      system_memory_state_desc AS [System Memory State]\n    FROM sys.dm_os_sys_memory WITH (NOLOCK) OPTION (RECOMPILE);","_id":"f2vDA04quYNM4Kxt","createdAt":{"$$date":1522252049408},"updatedAt":{"$$date":1522252049408}}
{"name":"ringBuffers","text":"SELECT [ring_buffer_type]\n      , ISNULL( COUNT(buffer.[ring_buffer_type] ), 0 ) AS [type_count]\n    FROM sys.dm_os_ring_buffers buffer\n    CROSS JOIN sys.dm_os_sys_info info\n    GROUP BY [ring_buffer_type];","_id":"hnZyyz2kZWsRtAqv","createdAt":{"$$date":1522252049416},"updatedAt":{"$$date":1522252049416}}
