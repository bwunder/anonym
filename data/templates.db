{"name":"databases","template":"SELECT DB_NAME([database_id]) AS [Database Name], [file_id], [name],\n      physical_name, [type_desc], state_desc, is_percent_growth, growth,\n          CONVERT(bigint, growth/128.0) AS [Growth in MB],\n      CONVERT(bigint, size/128.0) AS [Total Size in MB]\n    FROM sys.master_files WITH (NOLOCK)\n    ORDER BY DB_NAME([database_id]), [file_id] OPTION (RECOMPILE);","_id":"2717prfBTgE3Vuqz","createdAt":{"$$date":1516736911930},"updatedAt":{"$$date":1516736911930}}
